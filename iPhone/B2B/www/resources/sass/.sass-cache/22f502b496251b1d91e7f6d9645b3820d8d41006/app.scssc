/* VENDOR ************************************************************* */


    /* IOS5 TOGGLE ******************************************************** */
    .ios5_toggle {
        .x-toggle {
            float:right;
            top:.45em;
            right:1.8em;
            border: 0.1em solid #888;
            width: 4.7em;
            height:1.7em;
            border-radius: 0;
        }
        .x-thumb{
            width: 1.6em;
        }
        .x-thumb:before {
            width: 1.7em;
            height:1.7em;
            top:  0 !important;
            left: -.06em;
            background-color:  $beer-secondary-color !important;
            background-image: none;
            border-radius: 0;
        }

        .x-toggle-on {
            background-color: $beer-main-color;
            background-image: none;
        }

        .x-toggle-on-before{
            position:absolute;
            font-size: 1.0em;
            font-weight: bold;
            font-family: Arial, Helvetica, sans-serif;
            letter-spacing:-.05em;
            top: .28em;
        }            
        .x-toggle-on:before {
            @extend .x-toggle-on-before;
            content:'ON';
            color: #FFF;
            left: 1em;
        }
        .x-toggle-off {
            background-color: #FAFAFA !important;
            background-image: none;
        }
        .x-toggle-off:before {
            @extend .x-toggle-on-before;
            content:'OFF';
            color: #999;
            left: 2em;
        }
    }

    /* STAR RATING ******************************************************** */

    /* Base Rating Control styles */
    .x-rating-field {
    }

    .x-rating-inner {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);
    }

    .x-rating-item {
        float: left;
    }

    .x-rating-clear {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
        float: left;
    }

    .x-item-disabled .x-rating-clear,
    .x-item-disabled .x-rating-item {
        opacity: 0.4;
    }

    /*  Star Rating */

    .x-rating-star-hover {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
    }

    .x-rating-star {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
    }

    /* Multi-color Star Rating */

    .x-rating-star-color div{
        display: block;
        float: left;
        text-indent: -9999px;
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        background-color: #686868;
        width: 32px;
        height:32px;
        margin-top:2px;
        margin-right: 2px;
    }

    /* Image-less Star Rating */
    .x-imageless-rating .x-rating-inner > div
    {
        width: 1.1em;
        font-size: 1.8em;
        margin-top: 2px;
        color: transparent;
    }

    .x-imageless-star:before
    {
        -webkit-background-clip: text;
        content: "\2605";
        background-image: -webkit-linear-gradient(top, #E9E9E9 0%,#BCBCBC 100%);

    }

    .x-imageless-hover:before
    {
        -webkit-background-clip: text;
        content: "\2605";
        background-image: -webkit-linear-gradient(top, #ffc578 0%,#fb9d23 100%);
        text-shadow: 0 0 .2em #fb9d23;
    }

    /************ STAR RATING CUSTOM ****************/

    .x-starrating {
        border-radius: 20px;
        border: 15px solid white;
        background-color: red;
        width: 40px;
        height: 40px;
        margin-top: 7px;
    }

    .x-starrating-hover {
        border-radius: 20px;
        border: 15px solid white;
        background-color: green;
        width: 40px;
        height: 40px;
        margin-top: 7px;
    }

    .x-starrating-value {
        background-image: url('data:image/jpeg;base64,');
        background-position: center;
        background-repeat:no-repeat;
        background-size: 100% 100%;
        width: 40px;
        height:40px;
        margin-top: 2px;
    }

    .x-rating-startvalue {
        background-image: url('data:image/jpeg;base64,');
        background-position: center;
        background-repeat:no-repeat;
        background-size: 100% 100%;
        width: 40px;
        height:40px;
        margin-top: 2px;
        float: left;
    }

    .x-rating-endvalue {
        background-image: url('data:image/jpeg;base64,');
        background-position: center;
        background-repeat:no-repeat;
        background-size: 100% 100%;
        width: 40px;
        height:40px;
        margin-top: 2px;
        float: left;
    }

