//tabbar
.x-tab {
    margin: 0 1px;
}

.x-tab-share {
    @include background-gradient(#eee, matte);

    .x-button-label {
        color: #000;
        text-shadow: 0 0 0;
    }

    &.x-tab-active {
        background-image: none !important;
        background-color: #eee !important;
        border-color: #ccc;
    }
}

//browse index
