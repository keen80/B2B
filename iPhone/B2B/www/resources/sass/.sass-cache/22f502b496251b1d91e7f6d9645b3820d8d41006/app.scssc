    /* MIXINS --------------------------------------------------------------- */

        @mixin rounded_list {
            .x-list-item-label{
                background: #F9F9F9;
                border: 1px solid #CCC;
                margin: 4px;
                border-radius: 10px;
            }
        }

        @mixin light-border{
            border: 1px solid #EEE;
        }

        @mixin dark-border{
            border: 1px solid #AAA;
        }

        @mixin indent-list64{
            margin-left: 58px;
            margin-top: 4px;
        }

        @mixin indent-list32{
            margin-left: 38px;
            margin-top: 2px;
        }


/* LAYOUT ***************************************************************** */
    
    html, body {
        height: 100%;
        background-color: #FFF;
        overflow: hidden;
        margin: 0;
    }

    .clear { clear:both;}

    .x-slidenavigation-container{
        -webkit-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
        box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
    } 

    #notificationbutton {
        height: 32px;
        border-left: 1px solid lighten($beer-main-color, 3);
        -webkit-border-radius: 0;
        border-radius: 0;
    }

    .infobar{
        font-size: 14px;
        color: #666;
        background: #FFD;
        border-bottom: 1px solid #999;
    }

    #mylatestdrink{
        padding: 10px;
        border-bottom: 2px solid #AAA;
        background: #FFF;
    }

    /* SLIDER ------------------------------------------------------------- */

    #appslider {
        #slider_left{
            .x-list-item-label {
                width: 190px;
                border: 1px solid #CCC;
                margin: 4px;
                border-radius: 10px;
                .slider_item_icon{
                    padding-left: 30px;
                    background-size: 26px;
                    background-repeat: no-repeat;
                    background-position: left;
                    background-image: url("../icons/beer.png");
                }
                .slider_bottle{
                    background-image: url("../icons/drink_beerbottle.png");
                }
                .slider_whatsnew {
                    background-image: url("../icons/splash_star.png");
                }
                .slider_terms {
                    background-image: url("../icons/document.png");
                }
                .slider_aboutus {
                    background-image: url("../icons/target.png");
                }
            }
        }
    }

    #slider_logo {
        padding-top: 2px
    }

    #slider_toolbar{
        background: #FFF;
    }


/* SPLASH SCREEN ********************************************************** */

    #appLoadingIndicator, #logo_claim, #logo_footer {
