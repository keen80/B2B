    /* MIXINS --------------------------------------------------------------- */

        @mixin rounded_list {
            .x-list-item-label{
                background: #F9F9F9;
                border: 1px solid #CCC;
                margin: 2px;
                border-radius: 6px;
                padding: 6px 10px;
            }
            .x-list{
                background: transparent;
            }
        }

        @mixin light-border{
            border: 1px solid #EEE;
        }

        @mixin dark-border{
            border: 1px solid #AAA;
        }

        @mixin small-subtle{
            font-size: $fontsize_small;
            font-weight: normal;
            color: #666;
        }

        @mixin xsmall-subtle{
            font-size: $fontsize_xsmall;
            font-weight: normal;
            color: #AAA;
        }

        @mixin indent-list72{
            margin-left: 72px;
            margin-top: 2px;
        }

        @mixin indent-list64{
            margin-left: 58px;
            margin-top: 4px;
        }

        @mixin indent-list32{
            margin-left: 38px;
            margin-top: 2px;
        }

         @mixin indent-list0{
            margin-left: 0px;
            margin-top: 2px;
        }

        @mixin field_nationality($selector) {
            .field_nationality_#{$selector} {
                background: url("../flags/#{$selector}.png")  no-repeat 98% center !important;
            }
        }

        @mixin beer-iconmask($name) {
          .x-tab .x-button-icon.#{$name},
          .x-button .x-button-icon.x-icon-mask.#{$name} {
            -webkit-mask-image: theme_image($theme-name, "beer/" + $name + ".png");
          }
        }

        @mixin no-select {
            -webkit-touch-callout: none;
            -webkit-user-select: none;
            user-select: none;
        }

/* COMPONENTS ************************************************************* */

    .list-empty-text{
        padding: 20px;
        margin: 20px auto;
        text-align: center;
        font-weight: bold;
    }

    /* SMALL LIST  */

    .small-list{
        min-height: 36px;
        font-size: $fontsize_small_px;
        .list-header{
            @include indent-list64;
            .time{
                position: relative;
                float: right;
                margin-top: 1px;
                padding: 0px 2px 2px 14px;
                color: #BBB;
                background: url("../icons/time_clock_gray.png") no-repeat top left;
                background-size: 14px;
            }
            .info{
                font-weight: bold;
                .avatar{
                    position: absolute;
                    top: 10px;
                    left: 12px;
                    -webkit-border-radius: 5px;
                    border-radius: 5px;
                    border: 1px solid #CCC;
                }
            }
        }

        .list-text{
            @include indent-list64;
            word-wrap: break-word;
            margin-bottom: 4px;
            clear:both;
            color: #999;
            font-size: 11px;
        }

        .photo{
            clear: both;
            -webkit-border-radius: 5px;
            border-radius: 5px;
            width: 96%;
            @include dark-border;
        }

        .list-header-small{
            @extend .list-header;
            @include indent-list32;
        }

        .list-footer{
            .subtext{
                color: #AAA;
                font-size: $fontsize_small_px;
                position: relative;
                padding: 6px 10px;
            }
            .location-category{
                @extend .subtext;
                @include indent-list32;
                float: left;
            }
            .lowerbox{
                padding: 1px 10px 0px 30px;
                margin: 1px;
                @include dark-border;
                background-size: 18px;
                -webkit-border-radius: 5px;
                border-radius: 5px;
                opacity: 0.3;
                position: relative;
                float: right;
            }
            .like{
                @extend .lowerbox;
                background: url("../icons/smiley_laughing.png") no-repeat top left;
            }
            .like:active{
                opacity: 1 !important;
            }
            .drinkins{
                @extend .lowerbox;
                background: url("../icons/beer.png") no-repeat top left;
            }
            .drinkins:active{
                opacity: 1 !important;
            }
        }
