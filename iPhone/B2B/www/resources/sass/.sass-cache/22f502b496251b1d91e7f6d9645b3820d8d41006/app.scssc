/* VENDOR ***************************************************************** */


    /* IOS5 TOGGLE ******************************************************** */
    .ios5_toggle {
        .x-toggle {
            float:right;
            top:.45em;
            right:1.8em;
            border: 0.1em solid #888;
            width: 4.7em;
            height:1.7em;
            border-radius: 0;
        }
        .x-thumb{
            width: 1.6em;
        }
        .x-thumb:before {
            width: 1.7em;
            height:1.7em;
            top:  0 !important;
            left: -.06em;
            background-color:  $beer-secondary-color !important;
            background-image: none;
            border-radius: 0;
        }

        .x-toggle-on {
            background-color: $beer-main-color;
            background-image: none;
        }

        .x-toggle-on-before{
            position:absolute;
            font-size: 1.0em;
            font-weight: bold;
            font-family: Arial, Helvetica, sans-serif;
            letter-spacing:-.05em;
            top: .28em;
        }            
        .x-toggle-on:before {
            @extend .x-toggle-on-before;
            content:'ON';
            color: #FFF;
            left: 1em;
        }
        .x-toggle-off {
            background-color: #FAFAFA !important;
            background-image: none;
        }
        .x-toggle-off:before {
            @extend .x-toggle-on-before;
            content:'OFF';
            color: #999;
            left: 2em;
        }
    }

    /* STAR RATING ******************************************************** */

    /* Base Rating Control styles */
    .x-rating-field {
    }

    .x-rating-inner {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);
    }

    .x-rating-item {
        float: left;
    }

    .x-rating-clear {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
        float: left;
    }

    .x-item-disabled .x-rating-clear,
    .x-item-disabled .x-rating-item {
        opacity: 0.4;
    }

    /*  Star Rating */

    .x-rating-star-hover {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
    }

    .x-rating-star {
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        width: 32px;
        height:32px;
    }

    /* Multi-color Star Rating */

    .x-rating-star-color div{
        display: block;
        float: left;
        text-indent: -9999px;
        background-image: url('data:image/png;base64,');
        background-position: center;
        background-repeat:no-repeat;
        background-color: #686868;
        width: 32px;
        height:32px;
        margin-top:2px;
        margin-right: 2px;
    }

    /* Image-less Star Rating */
    .x-imageless-rating .x-rating-inner > div
    {
        width: 1.1em;
        font-size: 1.8em;
        margin-top: 2px;
        color: transparent;
    }

    .x-imageless-star:before
    {
        -webkit-background-clip: text;
        content: "\2605";
        background-image: -webkit-linear-gradient(top, #E9E9E9 0%,#BCBCBC 100%);

    }

    .x-imageless-hover:before
    {
        -webkit-background-clip: text;
        content: "\2605";
        background-image: -webkit-linear-gradient(top, #ffc578 0%,#fb9d23 100%);
        text-shadow: 0 0 .2em #fb9d23;
    }

    /************ STAR RATING CUSTOM ****************/

    #beerrating{
        .x-rating-inner {
            margin: 0 auto;
            width: 280px;
        }
         .x-starrating {
        border-radius: 20px;
        border: 15px solid white;
        background-color: $beer-secondary-color;
        width: 40px;
        height: 40px;
        margin-top: 5px;
        }

        .basestarvalue{
            width: 40px;
            height: 40px;
            background-size: 100% 100%;
        }

        .x-starrating-hover {
            @extend .basestarvalue;
            border-radius: 20px;
            border: 15px solid white;
            background-color: $beer-main-color;
            margin-top: 5px;
        }

        .x-starrating-value {
            @extend .basestarvalue;
            background-image: url('data:image/jpeg;base64,');
            background-position: center;
            background-repeat:no-repeat;
            margin-top: 2px;
        }

        .x-rating-startvalue {
            background: url('../icons/rate_down.png') no-repeat center center;
            @extend .basestarvalue;
            margin-top: 2px;
            float: left;
        }

        .x-rating-endvalue {
            background: url('../icons/rate_up.png') no-repeat center center;
            @extend .basestarvalue;
            margin-top: 2px;
            float: left;
        }
    }
   

/************************************************/

.place-detail-info {
    position: relative;
    border: 1px solid #DDD;
    border-radius: 10px;
    padding: 10px;
}

/* Modal Rating */

.x-modal-rating {
    background: transparent !important;
    margin-top: -1em;
}

.rating-desc {
    color: #fff;
    margin-top: 2.7em;
}

.cutout {
  -webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2), inset 0 5px 5px rgba(0, 0, 0, 0.6), inset 0 1px 5px rgba(0, 0, 0, 0.6);
  box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2), inset 0 5px 5px rgba(0, 0, 0, 0.6), inset 0 1px 5px rgba(0, 0, 0, 0.6);
}

.cutout-deep {
  -webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2), inset 0 10px 20px rgba(0, 0, 0, 0.6), inset 0 1px 20px rgba(0, 0, 0, 0.6);
  box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2), inset 0 10px 20px rgba(0, 0, 0, 0.6), inset 0 1px 20px rgba(0, 0, 0, 0.6);
}

/* Fixes TO DO, questo fa schifo */
#ext-button-1,
#ext-button-3,
#ext-button-4,
#ext-button-5,
 { margin: 5px; }

 /* CALLOUT */
.callout{
    position: relative;
    margin-top: -6px;

    .calloutTip1{
        height: 0;
        width: 0;
        border-bottom: 12px solid #DDD;
        border-left: 12px dotted transparent;
        border-right: 12px dotted transparent;
        left: 0px;
        top: 0px;
        margin-left: 20px;
        z-index: 10;
    }
    .calloutTip2{
        position: relative;
        left: -10px;
        top: 2px;
        height: 0;
        width: 0;
        border-bottom: 10px solid $infobar-color;
        border-left: 10px dotted transparent;
        border-right: 10px dotted transparent;
        z-index: 11;
        }
    .calloutContent{
        background: $infobar-color;
        border: solid 1px #DDD;
        position: relative;
        top: -1px;
        z-index: 9;
        padding: 4px 10px;
        text-align: left;
        font-size: $fontsize_xsmall;
        font-weight: normal;
        color: #666;
    }

}



/* SPIN BOTTLE ************************************************************* */

#bottlegame{
    background: transparent url(../bg/beergame.png) no-repeat center center;
}

#bottle {
    width: 300px; height: 100px;
    position: absolute;
    left: 10px; top: 150px;
    /* initial rotation */
    -webkit-transform: rotateZ(-50deg);
}
.normal {
    background: transparent url(../img/bottle.png) no-repeat left top;
}
.blurred {
    background: transparent url(../img/bottle_blur.png) no-repeat left top;
}
#shadow {
    width: 300px; height: 100px;
    left: 18px; top: 175px;
    position: absolute;
    background: transparent url(../img/bottle_shadow.png) no-repeat left top;
    -webkit-transform: rotateZ(-50deg);
}

#rules{
    font-weight: bold;
    padding: 200px 20px 20px 20px;
    h1{
        color: $beer-main-color;
        text-shadow: 0px 2px 2px #000;
        font-size: 30px;
    }

}

