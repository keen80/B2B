 itemTpl: [
            '<div class="item">',
                '<div class="person">',
                    '<div class="avatar" style="background-image:url({avatar});"></div>',
                    '<div class="name">{name}</div>',
                '</div>',
                '<div class="photo">',
                    '<div class="img" style="background-image:url({photo});"></div>',
                    '<div class="comment">{comment}</div>',
                '</div',
            '</div>'
        ].join('')
 *tabbar
.x-tab {
    margin: 0 1px;
}

.x-tab-share {
    @include background-gradient(#eee, matte);

    .x-button-label {
        color: #000;
        text-shadow: 0 0 0;
    }

    &.x-tab-active {
        background-image: none !important;
        background-color: #eee !important;
        border-color: #ccc;
    }
}
 *browse index
.browse-index {
    .x-item-selected {
        background: darken(#fff, 3);
    }

    .item {
        @include display-box;
        @include box-orient(horizontal);

        padding: .8em;

        img {
            display: none;
        }

        .person {
            padding-right: .5em;
            .avatar {
                width: 3em;
                height: 3em;
                background: #ddd;
                @include border-radius(.3em);
                background-size: auto 120%;
                background-position: center center;
            }

            .name {
                font-size: .8em;
                text-align: center;
                padding-top: .5em;
            }
        }

        .photo {
            @include box-flex(1);

            .img {
                width: 100%;
                height: 9em;
                background: #ddd;
                @include border-radius(.3em);
                background-size: 100% auto;
            }

            .comment {
                font-size: .8em;
                padding-top: .5em;
            }
        }
    }
}


