    /* MIXINS --------------------------------------------------------------- */

        @mixin rounded_list {
            .x-list-item-label{
                background: #F9F9F9;
                border: 1px solid #CCC;
                margin: 4px;
                border-radius: 10px;
            }
        }

        @mixin light-border{
            border: 1px solid #EEE;
        }

        @mixin dark-border{
            border: 1px solid #AAA;
        }


/* LAYOUT ***************************************************************** */
    
    html, body {
        height: 100%;
        background-color: #FFF;
        overflow: hidden;
        margin: 0;
    }

    .x-slidenavigation-container{
        -webkit-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
