/* COMPONENTS ************************************************************* */


    /* SMALL LIST  */

    .small-list{
        min-height: 30px;
        font-size: $fontsize_small_px;
        .list-header{
            @include indent-list64;
            .time{
                position: relative;
                float: right;
                margin-top: 1px;
                padding: 2px 2px 2px 20px;
                color: #BBB;
            }
            .info{
                font-weight: bold;
                .avatar{
                    position: absolute;
                    top: 12px;
                    left: 12px;
                    -webkit-border-radius: 5px;
                    border-radius: 5px;
                }
            }
        }
        .list-header-small{
            @extend .list-header;
            @include indent-list32;
        }
        .list-text{
            @include indent-list64;
            word-wrap: break-word;
            margin-bottom: 4px;
            clear:both;
            color: #999;
            font-size: 11px;
        }
        .list-text-notification{
            @include indent-list32;
            word-wrap: break-word;
            margin-bottom: 4px;
            clear:both;
            color: #999;
            font-size: 12px;
        }
        .photo{
            clear: both;
            -webkit-border-radius: 5px;
            border-radius: 5px;
            width: 96%;
            @include dark-border;
        }
        .list-footer{
            .subtext{
                color: #AAA;
                font-size: 11px;
                position: relative;
            }
            .location-category{
                @extend .subtext;
                @include indent-list32;
                float: left;
            }
            .lowerbox{
                padding: 1px 10px 0px 30px;
                margin: 1px;
                @include dark-border;
                background-size: 18px;
                -webkit-border-radius: 5px;
                border-radius: 5px;
                opacity: 0.3;
                position: relative;
                float: right;
            }
            .like{
                @extend .lowerbox;
                background: url("../icons/smiley_laughing.png") no-repeat top left;
            }
            .like:active{
                opacity: 1 !important;
            }
            .drinkins{
                @extend .lowerbox;
                background: url("../icons/beer.png") no-repeat top left;
            }
            .drinkins:active{
                opacity: 1 !important;
            }
        }
    }
