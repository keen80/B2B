        height: 32px;
        border-left: 1px solid lighten($beer-main-color, 3);
        -webkit-border-radius: 0;
        border-radius: 0;
    }

    .infobar{
        font-size: 14px;
        color: #666;
        background: #FFD;
        border-bottom: 1px solid #999;
    }

    /* SLIDER ------------------------------------------------------------- */

    #appslider {
        #slider_left{
            .x-list-item-label {
                width: 190px;
                border: 1px solid #CCC;
